{
	"name": "css475-music-library",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"local:dev": "NODE_ENV=local vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:generate": "drizzle-kit generate:pg",
		"db:introspect": "drizzle-kit introspect:pg",
		"db:build": "./scripts/build_db.sh",
		"db:relationships": "bun run ./scripts/create_relationship_inserts.ts ",
		"db:songs": "bun run ./scripts/create_kaggle_inserts.ts",
		"db:setup": "bun db:execute seed/create_tables.sql seed/insert_admin.sql",
		"db:execute": "bun run ./scripts/execute_sql.ts",
		"db:join-spotify": "bun run ./scripts/join_spotify_dataset.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.1",
